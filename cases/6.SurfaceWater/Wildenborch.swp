**********************************************************************************
* Part 12 Numerical solution of Richards' equation; criteria to reach convergence
*
 DTMIN = 1.0d-6
 DTMAX = 0.2      ! Maximum timestep, [ 0.01..0.5 d, R]
 THETOL    =  0.001  ! Maximum dif. water content between iterations, [1.d-5..0.01 cm3/cm3, R]
 GWLCONV   =  200.0  ! Maximum dif. groundwater level between iterations, [1.d-5..1000 cm, R]
 CritDevMasBalDt = 0.01  ! Critical Deviation in water balance of timestep [1.0d-5..100.0 cm, R]
*
* only for Swap303
MSTEPS = 10000000
*
* only for swap305
